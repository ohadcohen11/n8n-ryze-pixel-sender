{
	"name": "n8n-nodes-ryze-pixel-sender",
	"version": "0.1.7",
	"description": "Custom n8n node for handling deduplication and sending affiliate events to TrafficPoint pixel service",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"ryze",
		"pixel",
		"trafficpoint",
		"affiliate"
	],
	"author": {
		"name": "Ryze Beyond",
		"email": "ohad.cohen@ryzebeyond.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ohadcohen11/n8n-nodes-ryze-pixel-sender.git"
	},
	"engines": {
		"node": ">=20.15"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "echo 'Skipping lint'",
		"lint:fix": "echo 'Skipping lint'",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/MySqlApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/RyzePixelSender/RyzePixelSender.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"dependencies": {
		"mysql2": "^3.11.5"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
